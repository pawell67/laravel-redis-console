{
    "name": "pawell67/laravel-redis-console",
    "description": "A beautiful web-based Redis Console for Laravel. Browse keys, run commands, and monitor your Redis instance.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pawel Wankiewicz"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Pawell67\\RedisConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pawell67\\RedisConsole\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pawell67\\RedisConsole\\RedisConsoleServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "fix": "vendor/bin/pint"
    }
}